{
  "name": "@dawlet/algolet",
  "version": "0.4.0",
  "private": true,
  "license": "MIT",
  "author": "Yuichi Yogo",
  "main": "lib/main/index.js",
  "scripts": {
    "build": "run-p build:*",
    "build:main": "tsc",
    "build:renderer": "webpack --mode=production",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "electron": "electron .",
    "start": "run-p watch electron",
    "watch": "run-p watch:*",
    "watch:main": "tsc --watch",
    "watch:renderer": "webpack --mode=development --watch"
  },
  "build": {
    "appId": "com.electron.algolet-beta",
    "productName": "Algolet@Beta"
  },
  "dawlet": {
    "localizable": true
  },
  "description": "Algolet lets you write your own custom logic to generate music.",
  "dependencies": {
    "@dawlet/sheetmusic-viewer": "^0.4.0",
    "@dawlet/utils": "^0.4.0",
    "apollo-boost": "0.4.7",
    "graphql": "14.6.0",
    "monaco-editor": "0.20.0",
    "react": "16.13.1",
    "react-apollo": "3.1.4",
    "react-dom": "16.13.1",
    "react-monaco-editor": "0.35.0",
    "react-split-pane": "2.0.3"
  },
  "devDependencies": {
    "@types/jest": "25.2.1",
    "@types/react": "16.9.32",
    "@types/react-dom": "16.9.6",
    "@types/webpack": "4.41.10",
    "css-loader": "3.4.2",
    "electron": "8.2.0",
    "electron-builder": "22.4.1",
    "file-loader": "6.0.0",
    "html-webpack-plugin": "4.0.4",
    "jest": "25.2.7",
    "monaco-editor-webpack-plugin": "1.9.0",
    "npm-run-all": "4.1.5",
    "raw-loader": "4.0.0",
    "style-loader": "1.1.3",
    "ts-jest": "25.3.1",
    "ts-loader": "6.2.2",
    "ts-node": "8.8.2",
    "typescript": "3.8.3",
    "webpack": "4.42.1",
    "webpack-cli": "3.3.11"
  }
}
